{
  "name": "@jensmeindertsma/remark",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "concurrently -p name npm:build:*",
    "build:app": "remix build",
    "build:server": "tsx ./scripts/build-server.ts",
    "clean": "rimraf ./build/ ./public/build/",
    "database:generate": "prisma generate",
    "database:migrate": "prisma migrate dev",
    "dev": "remix dev --manual -c 'dotenv tsx ./server/main.ts'",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint . --fix",
    "lint:check": "eslint . --max-warnings 0",
    "start": "dotenv node ./build/server/main.js",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@conform-to/react": "^0.9.0",
    "@conform-to/zod": "^0.9.0",
    "@prisma/client": "5.5.2",
    "@remix-run/express": "^2.2.0",
    "@remix-run/node": "^2.2.0",
    "@remix-run/react": "^2.2.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "dotenv-cli": "^7.3.0",
    "express": "^4.18.2",
    "isbot": "^3.7.0",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "source-map-support": "^0.5.21",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@eslint/js": "^8.52.0",
    "@remix-run/dev": "^2.2.0",
    "@types/bcryptjs": "^2.4.5",
    "@types/compression": "^1.7.4",
    "@types/eslint": "^8.44.6",
    "@types/express": "^4.17.20",
    "@types/morgan": "^1.9.7",
    "@types/node": "^20.8.10",
    "@types/react": "^18.2.34",
    "@types/react-dom": "^18.2.14",
    "@types/source-map-support": "^0.5.9",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "chokidar": "^3.5.3",
    "concurrently": "^8.2.2",
    "esbuild": "^0.19.5",
    "eslint": "^8.52.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "globals": "^13.23.0",
    "prettier": "^3.0.3",
    "prisma": "^5.5.2",
    "rimraf": "^5.0.5",
    "tsx": "^3.14.0",
    "typescript": "^5.2.2"
  }
}
